div.nontel table tr th 
{
	padding: 5px;
}

div.RadGrid_test table
{
	font-size: .75em;
	margin-bottom: 1.25em;
    background: #fff;
}

div.RadGrid_test table td,
div.RadGrid_test table th 
{
    vertical-align: middle;
    white-space: nowrap;
}

.main div.RadGrid_test table td,
.main div.RadGrid_test table th 
{
    white-space: normal;
}

div.RadGrid_test table tr th.rgHeader
{
	text-align: left;
	border-bottom: 1px solid #c4c4c4;
	background: #e9e9e9;
	font-weight: bold;
	font-size: 1.125em;
}

div.RadGrid_test table tr th a 
{
	color: #000;
}

div.RadGrid_test table tr td 
{
	padding: .416em;
	border-bottom: 1px solid #e2e2e2;
}

div.RadGrid_test table tr.rgRow td 
{
	background: #fff;
}

div.RadGrid_test table tr.rgAltRow td 
{
	background: #fafafa;
}

div.RadGrid_test table.rgDetailTable
{
	margin: -.416em 0;
	font-size: 1em;
	border: 0;
}

/*

div.RadGrid_test table.rgDetailTable tr th.rgHeader 
{
	background-color: #e9e9e9;
}

div.RadGrid_test table.rgDetailTable tr.rgRow td 
{
	background: #fff;
}

div.RadGrid_test table.rgDetailTable tr.rgAltRow td 
{
	background: #fafafa;
}

*/

div.RadGrid_test table tr th.rgSorted,
div.RadGrid_test table tr th.rgSortedASC,
div.RadGrid_test table tr th.rgSortedDESC
{
	background: #cdcdcd;
}

div.RadGrid_test table tr th.rgSortedASC,
div.RadGrid_test table tr th.rgSortedDESC 
{
    padding-right: 20px;
    background-image: url("../img/dropdown.gif");
    background-position: right;
    background-repeat: no-repeat;
}

div.RadGrid_test table tr th.rgSortedASC 
{
    background-image: url("../img/dropup.gif");
}

div.RadGrid_test table tr.rgRow td.rgSorted 
{
	background: #f3f3f3;
}

div.RadGrid_test table tr.rgAltRow td.rgSorted 
{
	background: #e9e9e9;
}

div.RadGrid_test table tr.rgCommandRow td.rgCommandCell 
{
	border-bottom: 0;
}

div.RadGrid_test td.jobRow 
{
	font-size: 1.142857em;
}

/* nested tables */

.rgDetailTable
{
	padding-bottom: 3em;
}

a.button,
a.delete,
.rgPagerButton
{
	white-space: nowrap;
}

.buttoned a.button,
.buttoned a.delete 
{
	padding-right: 15px;
	padding-left: 15px;
	font-size: 12px;
	line-height: 24px;
	color: #333;
	background: #c4c4c4 url("../img/glass.png") repeat-x 0 50%;
	border: 1px solid #5e5e5e;
	border-radius: 10px;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	text-decoration: none;
	white-space: nowrap;
}

a.delete 
{
	color: Red;
}

.rgPagerButton 
{
	padding-bottom: 5px !important; /* this fixes it */
	border: 1px solid #9e9e9e !important;
}

.rgPagerButton:hover 
{
	color: #fff;
	background-color: #757575;
	border: 1px solid #333 !important;
}

.buttoned a.button:hover {
	color: #fff;
	background-color: #757575;
}

.buttoned a.delete:hover {
	color: #fff;
	background-color: #d63333;
	border-color: #c00;
}

.RadGrid_test .rgExpand {
background-position:5px -496px;
}

.RadGrid_test .rgCollapse {
background-position:3px -444px;
}

/* quality control statuses */

div.RadGrid_test table tr.rgRow td.Viewed 
{
	padding: 0;
	width: 0;
	/* background: yellow; Mac wants Unviewed instead 1/5/11 */
}

div.RadGrid_test table tr.rgRow td.Unviewed,
div.RadGrid_test table tr.rgRow td.needsestimate,
div.RadGrid_test table tr.rgRow.Unviewed td,
div.RadGrid_test table tr.rgRow.needsestimate td
{
	padding: 0;
	width: 0;
	background: #ffe57f;
	text-align: center; /* for asap */
}

div.RadGrid_test table tr.rgAltRow td.Unviewed,
div.RadGrid_test table tr.rgAltRow td.needsestimate,
div.RadGrid_test table tr.rgAltRow.Unviewed td,
div.RadGrid_test table tr.rgAltRow.needsestimate td
{
	padding: 0;
	width: 0;
	background: #ffe57f; /* ffd633; */
	text-align: center; /* for asap */
}

div.RadGrid_test table tr.rgRow td.OnHold
{
	padding: 0;
	width: 0;
}

div.RadGrid_test table tr.rgRow td.NeedsFix,
div.RadGrid_test table tr.rgRow td.recut,
div.RadGrid_test table tr.rgRow.NeedsFix td,
div.RadGrid_test table tr.rgRow.recut td
{
	padding: 0;
	width: 0;
	color: #ff3333;
	background: #ff7f7f;
	text-align: center; /* for recut */
}

div.RadGrid_test table tr.rgAltRow td.NeedsFix,
div.RadGrid_test table tr.rgAltRow td.recut,
div.RadGrid_test table tr.rgAltRow.NeedsFix td,
div.RadGrid_test table tr.rgAltRow.recut td
{
	padding: 0;
	width: 0;
	color: #ff3333;
	background: #ff7f7f; /* ff3333; */
	text-align: center; /* for recut */
}

div.RadGrid_test table tr.rgRow td.Finished,
div.RadGrid_test table tr.rgRow td.approved,
div.RadGrid_test table tr.rgRow.Finished td,
div.RadGrid_test table tr.rgRow.approved td
{
	padding: 0;
	width: 0;
	background: #99cc7f;
	color: White !important; /* for asap in approved */
	text-align: center; /* for asap */
}

div.RadGrid_test table tr.rgAltRow td.Finished,
div.RadGrid_test table tr.rgAltRow td.approved,
div.RadGrid_test table tr.rgAltRow.Finished td,
div.RadGrid_test table tr.rgAltRow.approved td
{
	padding: 0;
	width: 0;
	color: White !important; /* for asap in approved */
	text-align: center; /* for asap */
	background: #99cc7f; /* 5cad33; */
}

/* -- yellow highlighting */

div.RadGrid_test table tr.rgRow td.unownedrequest,
div.RadGrid_test table tr.rgRow.unownedrequest td
{
    background: #ffc;
}

div.RadGrid_test table tr.rgAltRow td.unownedrequest, 
div.RadGrid_test table tr.rgAltRow.unownedrequest td
{
	background: #ffffa3;
}

/* override yellow highlighting with green for approved */

div.RadGrid_test table tr.rgRow.unownedrequest td.approved,
div.RadGrid_test table tr.rgAltRow.unownedrequest td.approved
{
    background: #99cc7f;
	color: White !important; /* for asap in approved */
}

/* -- past due */

div.RadGrid_test table tr.pastdue td,
div.RadGrid_test table tr.pastdue td a
{
	color: Red;
}

div.completed div.RadGrid_test table tr.pastdue td,
div.completed div.RadGrid_test table tr.pastdue td a 
{
	color: inherit;
}

/* sort direction */

.RadGrid_test .rgHeader .rgSortDesc {
background-position: 3px -198px;
height:10px;
}

.RadGrid_test .rgHeader .rgSortAsc {
background-position: 3px -248px;
height:10px;
}

/* paging dude at the bottom */

div.RadGrid_test .rgPageFirst {
background-position: 0 -550px;
}

div.RadGrid_test .rgPagePrev {
background-position: 0 -700px;
}

div.RadGrid_test .rgPageNext {
background-position: 0 -850px;
}

div.RadGrid_test .rgPageLast {
background-position: 0 -1000px;
}

div.RadGrid_test .rgPagePrev,
div.RadGrid_test .rgPageNext,
div.RadGrid_test .rgPageFirst,
div.RadGrid_test .rgPageLast,
div.RadGrid_test .rgExpand,
div.RadGrid_test .rgCollapse,
div.RadGrid_test .rgSortDesc,
div.RadGrid_test .rgSortAsc {
background-image: url("../img/WebResource.gif");
}

div.RadGrid_test table tfoot table 
{
	font-size: 1em;
	background: #f0f0f0;
	border-top: 1px solid #999;
	margin-bottom: 0;
}

div.RadGrid_test table tr td.right 
{
	text-align: right;
}

div.RadGrid_test .rgPageFirst,
div.RadGrid_test .rgPagePrev {
margin-right:1px;
}

div.RadGrid_test .rgPageFirst,
div.RadGrid_test .rgPagePrev,
div.RadGrid_test .rgPageNext,
div.RadGrid_test .rgPageLast {
height:22px;
vertical-align:top;
width:22px;
}

a.addtlProduction 
{
    padding-right: 20px;
    background: url("../img/music.png") right no-repeat;
}

/* estimate on request tab */

span.mandated,
span.requested
{
    padding: 3px 5px;
    color: #f66;
    border: 1px solid #f66;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

span.requested 
{
    color: #f90;
    border-color: #f90;
}

div.RadGrid_test .rgSelectedRow td 
{
    background: #ffc !important;
}