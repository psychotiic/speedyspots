using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace SpeedySpots.Objects
{
    [Serializable]
    public class SpotFee
    {
        private int                                 m_iIASpotFeeID = 0;
        public int                                  m_iIASpotFeeTypeID = 0;
        public decimal                              m_fFee = 0;

        public int IASpotFeeID
        {
            get { return m_iIASpotFeeID; }
            set { m_iIASpotFeeID = value; }
        }

        public int IASpotFeeTypeID
        {
            get { return m_iIASpotFeeTypeID; }
            set { m_iIASpotFeeTypeID = value; }
        }

        public decimal Fee
        {
            get { return m_fFee; }
            set { m_fFee = value; }
        }
    }
}