using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace SpeedySpots_Quickbook_Connector
{
    public class DayValue
    {
        private int m_iKey = 0;
        private string m_sValue = string.Empty;

        public DayValue(int iKey, string sValue)
        {
            m_iKey = iKey;
            m_sValue = sValue;
        }

        public int Key
        {
            get { return m_iKey; }
        }

        public string Value
        {
            get { return m_sValue; }
        }
    }
}
