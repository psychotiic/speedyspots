.print
{
	display: none;
}

body {
	background: #dbdbdb url("../img/pageHeaderMarginSmall.gif") top left repeat-x;
	font-size: 100%;
	font-family: "Trebuchet MS", Arial, Verdana, Helvetica, sans-serif;
}

h2 {
	font-size: 1.25em; /* 20px */
	line-height: 1.8em; /* 36px */
	padding-top: .55em; /* 11px */
	margin-bottom: .35em; /* 7px */
}

h3 {
	font-size: 1em; /* 16px */
	line-height: 1.125em; /* 18px */
	padding-top: .1875em; /* 3px */
	margin-bottom: .9375em; /* 15px */
}

p {
	font-size: .875em; /* 14px */
	line-height: 1.285714285714286em; /* 18px */
	padding-top: .285714285714286em; /* 4px */
	margin-bottom: 1em; /* 14px */
}

a {
	color: #06c;
	text-decoration: none;
}

a:hover {
	text-decoration: underline;
}

#wrapper {
	margin: 0 auto;
	max-width: 62.5em;
}

/* ---------- header ---------- */

#header {
	color: #fff;
	background: #fd0c41;
	height: 73px;
	border-bottom: 6px solid #700718;
}

#Master_divWhosOnBlock 
{
    float: left;
    margin-top: 2px;
    display: block;
    height: 71px;
    width: 177px;
}

#header h1#logo 
{
    float: left;
}

#header h1#logo a 
{
    display: block;
	text-indent: -9999px;
	background: url("../img/logo-sm.png") top left no-repeat;
	height: 79px;
	width: 260px;
}

#header #top {
	background: url("../img/bg-right.png") top right no-repeat;
	width: 100%;
	height: 73px;
}

/* ---------- main navigation ---------- */

#nav, #nav ul {
	padding: 0;
	margin: 0;
	list-style: none;
}

#nav 
{
    font-size: .75em;
    line-height: 1.8333em;
    padding: .333em;
    background: #a4a4a4;
}

#nav ul 
{
    padding: 0 .833em;
    background: #767676;
    border-radius: .8333em;
}

#nav a {
	display: block;
	width: 100%;
	text-align: center;
	color: #fff;
}

#nav li {
	float: left;
	width: 14.2857%;
}

#nav ul li:hover a,
#nav ul li.at a 
{
    text-decoration: none;
    background-color: #44b5de;
}

#nav ul li:hover ul a,
#nav ul li.at ul a
{
    background-color: #767676;
}

#nav li ul 
{
    padding: 0;
	position: absolute;
	width: 16.666%;
	left: -999em;
	border-radius: 0;
	z-index: 1000;
}

#nav li ul li 
{
    width: 100%;
}

#nav li ul a 
{
    text-align: left;
    padding-left: 10px;
    margin-left: -10px;
}

#nav li:hover ul a:hover,
#nav li.at ul a
{
    color: #fff;
    background-color: #44b5de;
}

#nav li:hover ul {
	left: auto;
	padding-left: 10px;
	border: 1px solid #44b5de;
}

/* ---------- body ---------- */

#body {
	color: #333;
	background: #fff; /* url("../img/baseline-18.png") top left; */
	padding: 18px;
	padding-bottom: 0;
}

/* ---------- layout ---------- */

.full 
{
	float: left;
	width: 100%;
}

.main {
	float: left;
	margin-right: .9%;
	width: 68.6%; /* was 65 for some reason */
}

.sidebar {
	float: right;
	margin-left: .9%;
	width: 29.6%;
}

/* ---------- Dashboard tabs ---------- */

#dashboard-tabs,
#dashboard-tabs li 
{
	margin: 0;
	padding: 0;
	list-style: none;
	font-size: .875em;
}

.tab-holder {
	padding-left: 2.5%;
	background: url("../img/tab-bg.png") left repeat-x;
}

#dashboard-tabs li {
	font-size: 1em;
}

.tab-holder
{
	margin-bottom: 1em;
}
    
#dashboard-tabs li a 
{
	float: left;
	display: block;
	padding: 0 1%;
	width: 17%;
	font-size: 1em;
	line-height: 2.428571em;
	background: #ccebff url("../img/glass.png") repeat-x 0 50%;
	border: 1px solid #7fccff;
	-webkit-border-top-left-radius: 5px;
-webkit-border-top-right-radius: 5px;
-moz-border-radius-topleft: 5px;
-moz-border-radius-topright: 5px;
border-top-left-radius: 5px;
border-top-right-radius: 5px;
}

#dashboard-tabs li.at a 
{
	font-weight: bold;
	background: #fff;
	border-bottom-color: #fff;
	color: #000;
	text-decoration: none;
}

/* sub navigation for admin */

#dashboard-subnav 
{
	margin-top: -1em;
margin-bottom: 18px;
padding: 10px;
border-bottom: 1px solid #7fccff;
font-size: .875em;
}

#dashboard-subnav li 
{
    float: left;
}

#dashboard-subnav li a {
display: block;
padding: 5px 10px;
margin-right: 10px;
-webkit-border-radius: 8px;
-moz-border-radius: 8px;
border-radius: 8px;
}

#dashboard-subnav li.at a {
background: #06c;
color: #fff;
font-weight: bold;
}

