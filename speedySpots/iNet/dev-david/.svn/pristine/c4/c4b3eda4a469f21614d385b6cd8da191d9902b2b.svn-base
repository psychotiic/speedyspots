}

/* FB4996 - progress indicator */

ul.checkout {
	font-size: 13px;
	line-height: 50px;
	margin-bottom: 18px;
}

ul.checkout li span.step span.classic 
{
	line-height: 1.5em;
}

ul.checkout li span.step a.tooltip:hover span 
{
	margin-left: 70px;
}

ul.checkout li {
	float: left;
	display: block;
	color: #999;
	background: url("../img/checkout-bg.png") top no-repeat;
}

ul.checkout li a 
{
	color: #999;
}

ul.checkout li.first {
	background: url("../img/checkout-bg.png") top left no-repeat;
	padding-left: 7px;
}

ul.checkout li.last {
	background: url("../img/checkout-bg.png") top right no-repeat;
}

ul.checkout li.at 
{
    margin-left: -24px;
    padding-left: 24px;
    background-position: 0 50%;
}

ul.checkout li.at span.step 
{
    background-position: bottom right;
}

ul.checkout li.at a {
	color: #555555;
	font-weight: bold;
}

ul.checkout li.first.at 
{
    margin-left: 0;
    padding-left: 7px;
    background: url("../img/checkout-bg.png") bottom left no-repeat;
}

ul.checkout li.last.at span.step {
	background: url("../img/checkout-bg.png") 100% 50% no-repeat;
}

ul.checkout li.last span.step 
{
    background: none;
    padding-right: 27px;
}

ul.checkout li span.step {
	display: block;
	padding: 0 45px 0 19px;
	background: url("../img/checkout-div.png") top right no-repeat;
}

