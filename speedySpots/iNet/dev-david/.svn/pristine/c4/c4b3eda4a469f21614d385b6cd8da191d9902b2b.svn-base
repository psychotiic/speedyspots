	white-space: nowrap;
}

/* JQuery Multiselect drop down */
a.multiSelect {
	background: #FFF url("../img/dropdown.gif") right center no-repeat;
	border: solid 1px #BBB;
	padding-right: 20px;
	position: relative;
	cursor: default;
	text-decoration: none;
	color: black;
	display: -moz-inline-stack;
	display: inline-block;
	vertical-align: top;
	min-height: 20px;
	font-size: 12px;
}

a.multiSelect:link, a.multiSelect:visited, a.multiSelect:hover, a.multiSelect:active {
	color: black;
	text-decoration: none;
}

a.multiSelect span
{
	margin: 1px 0px 1px 3px;
	overflow: hidden;
	display: -moz-inline-stack;
	display: inline-block;
	white-space: nowrap;
}

a.multiSelect.hover {
	background-image: url("../img/dropdown_hover.gif");
}

a.multiSelect.active, 
a.multiSelect.focus {
	border: inset 1px #000;
}

a.multiSelect.active {
	background-image: url("../img/dropdown_active.gif");
}

.multiSelectOptions {
	margin-top: -1px;
	overflow-y: auto;
	overflow-x: hidden;
	border: solid 1px #B2B2B2;
	background: #FFF;
	font-size: 12px;
}

.form-holder fieldset .inputgroup .multiSelectOptions LABEL {
	padding: 0px 2px;
	display: block;
	white-space: nowrap;
	text-align: left;
	margin-left: auto;
}

.form-holder fieldset .inputgroup .multiSelectOptions LABEL.optGroup
{
	font-weight: bold;
}

.form-holder fieldset .inputgroup .multiSelectOptions .optGroupContainer LABEL
{
	padding-left: 10px;
}

.form-holder fieldset .inputgroup .multiSelectOptions.optGroupHasCheckboxes .optGroupContainer LABEL
{
	padding-left: 18px;
}

.form-holder fieldset .inputgroup .multiSelectOptions input{
	vertical-align: middle;
}

.form-holder fieldset .inputgroup .multiSelectOptions LABEL.checked {
	background-color: #dce5f8;
}

.form-holder fieldset .inputgroup .multiSelectOptions LABEL.selectAll {
	border-bottom: dotted 1px #CCC;
}

.form-holder fieldset .inputgroup .multiSelectOptions LABEL.hover {
	background-color: #3399ff;
	color: white;
}

