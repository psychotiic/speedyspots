}

.sidebar ul#side-tabs li#tabRequest.at a 
{
	border-bottom-color: #fff;
}

.sidebar ul#side-tabs li#tabNotes.at a 
{
	border-bottom-color: #ffebcc;
}

.sidebar ul#side-tabs li#tabJob.at a,
.sidebar ul#side-tabs li#tabSched.selected a
{
