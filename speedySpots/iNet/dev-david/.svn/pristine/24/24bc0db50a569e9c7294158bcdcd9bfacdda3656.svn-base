body {
	background: #2872bd;
	color: #222;
	font-family: "Lucida Grande", Helvetica, Arial, Verdana, sans-serif;
	font-size: 11px;
	line-height: 1.7em;	
}

h2 {
	border-bottom: 1px solid #eee;
	color: #333;
	font-size: 18px;
	font-weight: bold;
	padding-bottom: 3px;
	margin-bottom: 1em;
}

a, a:visited, .RadGrid_Default td a {
	color: #2872bd;
	text-decoration: none;
}

.RadGrid_Default td a 
{
	text-decoration: underline;
}

a:hover, .RadGrid_Default td a:hover {
	color: #333;
}

